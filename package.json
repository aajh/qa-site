{
  "name": "qa-site",
  "version": "1.0.0",
  "description": "A question and answer site.",
  "repository": {
    "type": "git",
    "url": "https://github.com/aajh/qa-site.git"
  },
  "main": "dist/src/server.js",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "build": "npm run clean && tsc && webpack --config webpack.prod.config.js",
    "build:dev": "npm run clean && tsc",
    "start": "node -r source-map-support/register .",
    "start:dev": "npm-run-all build:dev start",
    "start:test": "PORT=3333 NODE_ENV=test npm-run-all build:dev db:test:up start",
    "watch": "nodemon --watch src -e ts,tsx,js --exec npm run start:dev",
    "clean": "rm -rf dist",
    "test": "cypress run",
    "cy:open": "cypress open",
    "ci": "start-server-and-test start:test http://localhost:3333",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "db:test:up": "db-migrate up -e test"
  },
  "author": "Aapo Hanski",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bootstrap": "^4.3.2",
    "@types/classnames": "^2.2.10",
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/connect-pg-simple": "^4.2.0",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.0",
    "@types/pg": "^7.14.3",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.4",
    "@types/serialize-javascript": "^1.5.0",
    "@types/uuid": "^7.0.2",
    "@types/webpack": "^4.41.12",
    "@types/webpack-dev-middleware": "^3.7.0",
    "@types/webpack-hot-middleware": "^2.25.2",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "css-loader": "^6.7.3",
    "cypress": "^4.5.0",
    "db-migrate": "^0.11.10",
    "db-migrate-pg": "^1.2.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "mini-css-extract-plugin": "^2.7.2",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "source-map-loader": "^4.0.1",
    "start-server-and-test": "^1.11.0",
    "style-loader": "^1.1.4",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^9.4.2",
    "typescript": "^3.8.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.3.5",
    "bcrypt": "^4.0.1",
    "bootstrap": "^4.4.1",
    "classnames": "^2.2.6",
    "connect-pg-simple": "^6.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-promise-router": "^3.0.3",
    "express-session": "^1.17.1",
    "history": "^4.10.1",
    "immutable": "^4.0.0-rc.12",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.5.0",
    "lodash": "^4.17.15",
    "pg": "^8.0.3",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^5.5.3",
    "react-markdown": "^4.3.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "seamless-immutable": "^7.1.4",
    "serialize-javascript": "^3.0.0",
    "source-map-support": "^0.5.19",
    "uuid": "^7.0.3"
  }
}
